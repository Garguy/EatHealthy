<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.poweron_studio.eathealthy.GlutenFree"
    android:background="#CCCC99">

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/proteinwafflesButton"
        android:id="@+id/proteinwafflesButton"
        android:onClick="proteinwafflesOnClick"
        android:background="#0099CC"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/glutenfreepizzaButton"
        android:id="@+id/glutenfreepizzaButton"
        android:onClick="glutenfreepizzaOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/proteinwafflesButton"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/breadButton"
        android:id="@+id/breadButton"
        android:onClick="breadOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/glutenfreepizzaButton"
        android:layout_alignLeft="@+id/glutenfreepizzaButton"
        android:layout_alignStart="@+id/glutenfreepizzaButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/nutbreadButton"
        android:id="@+id/nutbreadButton"
        android:onClick="nutbreadOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/breadButton"
        android:layout_alignLeft="@+id/breadButton"
        android:layout_alignStart="@+id/breadButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/peanutbuttercookiesButton"
        android:id="@+id/peanutbuttercookieButton"
        android:onClick="peanutbuttercookieOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/nutbreadButton"
        android:layout_alignLeft="@+id/nutbreadButton"
        android:layout_alignStart="@+id/nutbreadButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/oatmealcookiesButton"
        android:id="@+id/oatmealcookiesButton"
        android:onClick="oatmealcookiesOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/peanutbuttercookieButton"
        android:layout_alignLeft="@+id/peanutbuttercookieButton"
        android:layout_alignStart="@+id/peanutbuttercookieButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/beanchiliButton"
        android:id="@+id/beanchiliButton"
        android:onClick="beanchiliOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/oatmealcookiesButton"
        android:layout_alignLeft="@+id/oatmealcookiesButton"
        android:layout_alignStart="@+id/oatmealcookiesButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/basilshrimpButton"
        android:id="@+id/basilshrimpButton"
        android:onClick="basilshrimpOnClick"
        android:background="#0099CC"
        android:layout_below="@+id/beanchiliButton"
        android:layout_alignLeft="@+id/beanchiliButton"
        android:layout_alignStart="@+id/beanchiliButton" />





</RelativeLayout>
