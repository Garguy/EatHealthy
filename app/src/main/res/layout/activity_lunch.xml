<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.poweron_studio.eathealthy.Lunch"
    android:background="#D1B37F">

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/greenpepperButton"
        android:id="@+id/greenpepperButton"
        android:onClick="greenpepperOnClick"
        android:background="#41924B"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/bisonButton"
        android:id="@+id/bisonButton"
        android:onClick="bisonOnClick"
        android:background="#41924B"
        android:layout_below="@+id/greenpepperButton"
        android:layout_alignLeft="@+id/greenpepperButton"
        android:layout_alignStart="@+id/greenpepperButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/fishButton"
        android:id="@+id/fishsoftButton"
        android:onClick="fishsoftOnClick"
        android:background="#41924B"
        android:layout_below="@+id/bisonButton"
        android:layout_alignLeft="@+id/bisonButton"
        android:layout_alignStart="@+id/bisonButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/redpearButton"
        android:id="@+id/redpearButton"
        android:onClick="redpearOnClick"
        android:background="#41924B"
        android:layout_below="@+id/fishsoftButton"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/creamyButton"
        android:id="@+id/creamyButton"
        android:onClick="creamyOnClick"
        android:background="#41924B"
        android:layout_below="@+id/redpearButton"
        android:layout_alignLeft="@+id/redpearButton"
        android:layout_alignStart="@+id/redpearButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/tangyButton"
        android:id="@+id/tangyButton"
        android:onClick="tangyOnClick"
        android:background="#41924B"
        android:layout_below="@+id/creamyButton"
        android:layout_alignLeft="@+id/creamyButton"
        android:layout_alignStart="@+id/creamyButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/chickenButton"
        android:id="@+id/chickenButton"
        android:onClick="chickenOnClick"
        android:background="#41924B"
        android:layout_below="@+id/tangyButton"
        android:layout_alignLeft="@+id/tangyButton"
        android:layout_alignStart="@+id/tangyButton"
        android:layout_marginBottom="5dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/greekButton"
        android:id="@+id/greekButton"
        android:onClick="greekOnClick"
        android:background="#41924B"
        android:layout_below="@+id/chickenButton"
        android:layout_alignLeft="@+id/chickenButton"
        android:layout_alignStart="@+id/chickenButton" />

</RelativeLayout>
